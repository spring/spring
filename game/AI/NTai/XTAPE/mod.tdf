	hold_pos=CORCOM, ARMCOM;
	interpolate_tag=B_RULE_EXTREME_NOFACT;
	antistall=7; // 3 and 7 are currently the only good options.
	MaxStallTime=-10;  // Hey, it's finally used! // higher = more lenient
