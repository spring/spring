	{
		energyIncome=1;
		energyStored=1;
		metalIncome=1;
		metalStored=1;
	}
}
[ECONOMY]
{
	[RULES]
	{
		power=0.8;
		mex=0.8;
		
		factorymetal=0.9;
		factoryenergy=0.9;
		factorymetalgap=10;
		
		energystorage=0.8;
		metalstorage=0.8;
		
		makermetal=0.9;
		makerenergy=0.7;
		
		[EXTREME]
		{
			factorymetal=0.99; // maxed out
			factoryenergy=0.99; // maxed out
			
			power=0.3;
			mex=0.3;
			
			makermetal=0.3;
			makerenergy=0.8;
		}
	}
}
