CC=i586-mingw32-g++
IFLAGS=-I../../../ -I../../../rts -I../../../rts/System -I../../../rts/System/creg -I../../../rts/Sim/Units -I../../../rts/Sim/Weapons
TARGET=KAI012-mingw-32.dll
BUILD_DIR=build-mingw-32

all:
	$(CC) -Wall -Wextra -c *.cpp $(IFLAGS)
	$(CC) -shared -o $(TARGET) *.o
	mv $(TARGET) $(BUILD_DIR)

clean:
	rm *.o
